    "method-override": "~2.1.1",
    "morgan": "~1.0.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "prismic-nodejs": "1.3.0",
    "pug": "^2.0.0-alpha6",
    "request": "^2.76.0",
    "serve-favicon": "~2.3.0",
    "sort-keys": "^1.1.2",
    "title-sort": "^1.0.1"
  },
  "repository": {
