{
  "name": "nurj",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently 'npm run watch' 'npm run server'",
    "watch": "node tasks/watch.js",
    "server": "node app.js",
    "lint": "eslint app.js config.js prismic-configuration.js",
    "test": "npm run lint",
    "sass": "node-sass src/stylesheets -o public/stylesheets",
    "js": "./node_modules/.bin/gulp",
    "build": "npm run js && npm run sass",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "body-parser": "~1.0.0",
    "chokidar": "^1.7.0",
    "concurrently": "^4.0.1",
    "debug": "~0.7.4",
    "errorhandler": "~1.1.1",
    "exit-hook": "^1.1.1",
    "express": "~4.13.4",
    "first-letter": "^1.0.2",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-transform": "^1.1.0",
    "is-alphanumeric": "^1.0.0",
    "jquery": "^3.2.1",
    "method-override": "~2.1.1",
    "morgan": "~1.0.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "prismic-nodejs": "1.3.0",
    "pug": "^2.0.0-alpha6",
    "request": "^2.76.0",
    "serve-favicon": "~2.3.0",
    "shelljs": "^0.7.8",
    "sort-keys": "^1.1.2",
    "tiny-lr": "^1.0.4",
    "title-sort": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com:nurj/nurj.git"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "browserify": "^13.0.1"
  }
}
